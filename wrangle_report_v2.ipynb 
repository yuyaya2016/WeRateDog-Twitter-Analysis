   "source": [
    "df_clean['tweet_id'] = df_clean['tweet_id'].astype(str)\n",
    "image_preds_clean['tweet_id'] = image_preds_clean['tweet_id'].astype(str)\n",
    "tw_df_clean['tweet_id'] = tw_df_clean['tweet_id'].astype(str)"
   ]
  },
